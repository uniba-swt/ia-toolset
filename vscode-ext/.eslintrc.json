{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": [0],
        "space-before-function-paren": [0],
        "padded-blocks": [0],
        "key-spacing": [0],
        "no-trailing-spaces": [0],
        "semi": [1],
        "no-use-before-define": [1],
        "no-unused-vars": [1],
        "lines-between-class-members": [1],
        "eol-last": [1],
        "quotes": [1]
    }
}
